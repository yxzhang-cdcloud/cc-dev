pipeline {
    agent any
 
    stages {
        stage('clone code') {
            steps {
                echo 'Hello World'
                sh 'echo "123123123"'
            }
        }
        stage('build') {
            steps {
                echo 'Hello World'
                sh 'echo "123123123"'
            }
        }
        stage('delploy') {
            steps {
                echo 'Hello World'
                sh 'echo "123123123"'
            }
        }
    }
}
